# Environment
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL="postgresql://postgres:Root@321@localhost:5432/winadeal?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# OTP
OTP_EXPIRES_IN=10
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Google Maps
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Razorpay
RAZORPAY_KEY_ID=your-razorpay-key-id
RAZORPAY_KEY_SECRET=your-razorpay-key-secret

# Firebase
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY=your-firebase-private-key
FIREBASE_CLIENT_EMAIL=your-firebase-client-email

# Frontend URLs
ADMIN_PANEL_URL=http://localhost:3000
CUSTOMER_WEB_URL=http://localhost:3001

# Email (Optional - for SendGrid/AWS SES)
EMAIL_FROM=noreply@winadeal.com
SENDGRID_API_KEY=your-sendgrid-api-key

# Sentry (Error Monitoring)
SENTRY_DSN=your-sentry-dsn

# App Configuration
MIN_ORDER_VALUE=50
DEFAULT_DELIVERY_RADIUS_KM=5
DEFAULT_COMMISSION_RATE=20
